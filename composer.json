{
    "name": "lakshmaji/razorpay",
    "description": "Razorpay payment integration for laravel",
    "keywords": [
        "Razorpay", "Laravel", "composer"
    ],
    "homepage" : "https://github.com/lakshmaji/razorpay",
    "support" : {
        "source" : "https://github.com/lakshmaji/razorpay",
        "issues" : "https://github.com/lakshmaji/razorpay/issues"
    },
    "require": {
        "razorpay/razorpay": "^2.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "lakshmaji",
            "email": "lakshmajee88@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Lakshmaji\\Razorpay\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lakshmaji\\Razorpay\\RazorpayServiceProvider"
            ],
            "aliases": {
                "Razorpay": "Lakshmaji\\Razorpay\\Razorpay"
            }
        }
    }
}
